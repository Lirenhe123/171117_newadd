<script type="text/javascript" src="js/jQuery/jquery.placeholder.min.js"></script>
<script>

var init = jQuery.extend({type:'1',flag:'0',proj:'0',market:'000',arg:'1'},{type:'1',flag:'0',proj:'0',market:'000',arg:'1'});
var typeList = [{n:'采购',c:'1',w:false},{n:'招标',c:'3',m1:true,w:true},{n:'竞买',c:'4',w:true},{n:'集中竞价',c:'6',m2:true},{n:'销售',c:'2',w:false},{n:'竞卖',c:'5',m1:true,m2:true},{n:'公示',c:'7',w:false},{n:'审前公示',c:'8',m1:true},

{n:'采购公示',c:'9',m2:true}

];
var flagList = [{n:'项目公告',c:'0'},{n:'中标公告',c:'2'},{n:'变更公告',c:'3'},{n:'补遗公告',c:'4'},{n:'结果补遗',c:'5'},{n:'流标公告',c:'6'}];
var projList = [{n:'全部',c:'0'},{n:'线上项目',c:'1',w:false},{n:'正式项目',c:'3',m1:true,w:true},{n:'模拟项目',c:'4',m2:true},{n:'线下项目',c:'2'}];
var marketList = [{n:'全部',c:'000'},{n:'哈尔滨铁路局',c:'B00'}];
var htmlString = "";
var clsname ="";
var fixTime = (1501486346870-(new Date()).getTime());
var mainDate=new Date();
var initList = function(id,args,list){
	htmlString = "";
	$.each(list, function(i, obj){
		clsname = "";
		if(obj.c==args){
			clsname="sign ";
		}
		if(jQuery.type(obj.w)==="boolean"){
			if(obj.w)
				clsname+="marginr5";
			else
				clsname+="marginr2";
		}
		htmlString+="<span class=\""+clsname+"\" onclick=\"setArgs(this,'"+id+"','"+obj.c+"')\">";
		if(jQuery.type(obj.m1)==="boolean"&&obj.m1){
			htmlString+="【";
		}
		htmlString+=obj.n;
		if(jQuery.type(obj.m2)==="boolean"&&obj.m2){
			htmlString+="】";
		}
		htmlString+="</span>";
	}); 
	$("#"+id).html(htmlString);
	$("#"+id+" >span").mouseover(function(){$(this).addClass("spanOver");}).mouseout(function(){$(this).removeClass("spanOver");});
	htmlString = null;
	clsname = null;
}
var setArgs = function(obj,id,value){
	$("#"+id+" >.sign").removeClass("sign");
	$(obj).addClass("sign");
	init[id.replace("List","")]=value;
	search(1);
}
var search = function(page){
	if(!/^[A-Za-z0-9\u4e00-\u9fa5]*$/.exec($("#searchKeyValue").val())){
		alert("对不起，搜索关键词只能由数字、字母和常见汉字组成。");
		$("#searchKeyValue").val("");
		return false;
	}
	if(!/^[A-Za-z0-9\u4e00-\u9fa5]*$/.exec($("#inforCode").val())){
		alert("对不起，要查询ID只能由数字、字母和常见汉字组成。");
		$("#inforCode").val("");
		return false;
	}
	if(($("#time0").val()!=null&&$("#time0").val()!="")&&($("#time1").val()!=null&&$("#time1").val()!="")){
		if($("#time0").val()>$("#time1").val()){
			alert("[起始时间]应该小于[结尾时间]");
			return false;
		}
	}
	window.location.href ="http://wz.harb.95306.cn:80/mainPageNoticeList.do?method=init&id="+init.type+init.flag+init.proj+init.market+init.arg+"&cur="+page+"&keyword="+encodeURI($("#searchKeyValue").val())+"&inforCode="+encodeURI($("#inforCode").val())+"&time0="+$("#time0").val()+"&time1="+$("#time1").val(); 
}
var checkPageNum = function(obj){
	if(/^\d*$/.exec($.trim(obj.value)))
		obj.value = $.trim(obj.value);
	else
		obj.value = "";
}
function updateTime(){
	mainDate.setTime((new Date().getTime())+fixTime);
	var str=mainDate.getFullYear()+'年 '+(mainDate.getMonth()+1)+'月'+mainDate.getDate()+'日'+'&nbsp;&nbsp;星期'+'日一二三四五六'.charAt(new Date().getDay())+'&nbsp;&nbsp;'+mainDate.getHours()+'时'+mainDate.getMinutes()+'分';
	$("#jnkc").html(str);
	}
$(document).ready(function(){
	$(".searchGrayBox1 input").placeholder();
});
window.onload = function(){
	updateTime(fixTime);
	setInterval("updateTime();",1000);
	initList("typeList",init.type,typeList); 
	initList("flagList",init.flag,flagList); 
	initList("projList",init.proj,projList); 
	initList("marketList",init.market,marketList);  
	$("#selectIconBt").click(function(){if($(this).data("v")=="0"){$('#optionSelects').show('normal');$(this).removeClass().addClass("fold");$(this).data("v","1");init.arg='1';}else{$('#optionSelects').hide('normal');$(this).removeClass().addClass("unfold");$(this).data("v","0");init.arg='0';}}).mouseover(function(){if($(this).data("v")=="0"){$(this).removeClass().addClass("unfoldOver");}else{$(this).removeClass().addClass("foldOver");}}).mouseout(function(){if($(this).data("v")=="0"){$(this).removeClass().addClass("unfold");}else{$(this).removeClass().addClass("fold");}});
	if(init.arg!="1"){$('#optionSelects').hide();$("#selectIconBt").removeClass().addClass("unfold");$("#selectIconBt").data("v","0");}
}
</script>
