<script type="text/javascript">
	var fixTime = (1501486382579-(new Date()).getTime());
	var mainDate=new Date();
	updateTime(fixTime);
	setInterval("updateTime();",1000);
	$(document).ready(function(){
		 $(".noticeBox").height($(".divAbs").outerHeight()+$(".topTlt").outerHeight()+40);
	});
	function updateTime(){
		mainDate.setTime((new Date().getTime())+fixTime);
		var str=mainDate.getFullYear()+'年 '+(mainDate.getMonth()+1)+'月'+mainDate.getDate()+'日'+'&nbsp;&nbsp;星期'+'日一二三四五六'.charAt(new Date().getDay())+'&nbsp;&nbsp;'+mainDate.getHours()+'时'+mainDate.getMinutes()+'分';
		$("#jnkc").html(str);
		}
	
	function downLoad(i){
		
		var pattrn=/^[0-9]{1}(4|5)$/g;
  		document.form1.fileID.value=$('#fileID'+i).val();
  		document.form1.action="/downloadfile.do?flag=1";
	  	document.form1.submit();
  	}
	
	
  	//查看可售包件
  	function readSell(projID){
  		
  		if("未登录,不允许参与该项目"!="ok"){
  			alert("未登录,不允许参与该项目");
  			return;
  		}
  		if(''==''){
  		 	if(''=='3'){
  				var url = "loadMenu.do?modelNo=3";
  				window.location.href="http://wz.harb.95306.cn:80/"+url;
  			}else{
  				var url = "/pack/buy.do?rbt=cls&projId="+projID;
				var win = "top=0,left=0,width=1044,height=650,toolbar=0,menubar=0,location=0,directories=0,scrollbars=1,status=1,resizable=1";
				window.open(url,"readSell",win);
  			}
  		}else{
  			var url = "/sealed/supplier/toapply.do?id="+projID;
			var win = "top=0,left=0,width=1044,height=650,toolbar=0,menubar=0,location=0,directories=0,scrollbars=1,status=1,resizable=1";
			window.open(url,"sealedApply",win);
  		}
  	}
  	$(document).ready(function(){
  		
		$("#partake").click(function(){
			readSell('OB002017072704302373');
		});
		$("#colse_btn").click(function(){
			top.window.close();
		});	
		$("#print_btn").click(function(){
			preview();
		});		
		
	});
	
  	function preview(){
  		
  		$("#colse_btn").hide();
  		$("#print_btn").hide();
  		$("#partake").hide();
  		
  		if($.browser.msie&&$.browser.version=="6.0"){
  			$(".noticeBox").css("width","100%");
  			$(".noticeBox *").css("font-size","12px");
  			$(".noticeBox *").css("line-height","20px");
  			
  		}
  		var arr = $("img").get();
  		var n = arr.length;
  		for(i=1;i<n;i++){
  		 if(arr[i].width>700)
			arr[i].style.width="100%";
  		}
  		$("table").css("width","98%").css("margin","10px auto");
  		
  		
  		
  		var bdhtml=window.document.body.innerHTML;
  		var sprintstr="<!--startprint-->";
  		var eprintstr="<!--endprint-->";
  		var prnhtml=bdhtml.substring(bdhtml.indexOf(sprintstr)+17);
  		prnhtml=prnhtml.substring(0,prnhtml.indexOf(eprintstr))+location.href;
  		window.document.body.innerHTML=prnhtml;
  		window.print();
  	}
  	$(function(){
  		$("a[name^='docFile']").click(function(){
			var id=$(this).attr('name').substring(7,100); 
			var win = "top=0,left=0,width=1044,height=650,toolbar=0,menubar=0,location=0,directories=0,scrollbars=1,status=1,resizable=1";
  			var url = "notification/bidDocDownload.do?method=openBidDocDownload&documentId="+id;
  			window.open(url,"",win);
  		});
 	});
</script>
