        function redirectTo(type) 
        {
            //跳转到建行网银支付页面代码
            var ORDERID = '201711220923248571';
            var PAYMENT = $("#" + 'ctl00_ContentPlaceHolder1_DB_BidDeposit').html();
            var PROJECTNO = $("#" + 'ctl00_ContentPlaceHolder1_DB_TenderNo').html();
            var PROINFO = $("#" + 'ctl00_ContentPlaceHolder1_DB_TenderName').html();
            var REGINFO = $("#" + 'ctl00_ContentPlaceHolder1_DB_EnterpriseName').html();

            if (parseFloat(PAYMENT) <= 0) {
                alert("此项目不需要缴存投标保证金！");
                return;
            }

            var url = "BulletinBrowse.aspx/SaveOrderID";
            execJqueryJsonAjax(url, "{orderId:\"" + ORDERID + "\",projectno:\""+PROJECTNO+"\"}", function(res) {

                if (res.d != "") {
                    alert(res.d);
                    return;
                }
                var redirectUrl = "http://www.hcsggzy.com/IBank/Redirect.aspx?ORDERID="
                                            + ORDERID + "&PAYMENT=" + PAYMENT + "&PROJECTNO=" + PROJECTNO + "&PROINFO=" + escape(PROINFO) + "&REGINFO=" + escape(REGINFO) + "&type=" + type;
                window.open(redirectUrl);

            }, null);
        }
    </script>

        
