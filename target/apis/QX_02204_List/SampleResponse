<script type="text/javascript">



       function gotonum(){

               var Nowpage=document.getElementById("nowPage").value;

                       if(checkNum(Nowpage)){

                               if(Nowpage<=1){

                                       window.location = "index.html";

                               }

                               else if(Nowpage>8){

                                       Nowpage = 8;

                                       window.location="index_"+Nowpage+".html";

                               }

                               else{

                                       window.location="index_"+Nowpage+".html";

                               }

               

                       }

                       else{

                                       window.location="index.html";

                       }                                

       

       }

   function checkNum(str){ 

                       //如果为空，则通过校验 

               if(str == "") 

                       return true; 

               if(/^(\-?)(\d+)$/.test(str)) 

                  return true; 

               else 

                  return false; 

        } 

</script>

