                  <td height="50" align="center"><script language="javascript">

 function showListPage(recordCount, pageCount, pageNow){

        if(pageCount<1) 

               pageCount =0;

        if(pageNow<1)  

               pageNow = 0;

        str = '共 <B>'+recordCount+'</B> 条记录，当前 <B>'+pageNow+'/'+pageCount+'</B> 页';




        if(pageNow<=1)

               str += " 首页 ";

        else

               str += " <A href='index.html'><font color='black'>首页</font></A> ";




        if(pageNow==2)

               str += " <A href='index.html'><font color='black'>上一页</font></A> ";        

        else if(pageNow<=1)

               str += " 上一页 ";

        else

               str += " <A href='index_"+(pageNow-1)+".html'><font color='black'>上一页</font></A> ";




        if(pageNow>=pageCount)

               str += " 下一页 ";

        else

               str += " <A href='index_"+(pageNow+1)+".html'><font color='black'>下一页</font></A> ";




        if(pageNow>=pageCount)

               str += " 尾页 ";

        else

               str += " <A href='index_"+pageCount+".html'><font color='black'>尾页</font></A> ";




        return str;

}




</SCRIPT>


<SCRIPT LANGUAGE="JavaScript">document.write(showListPage(141, 8,2));</SCRIPT>
