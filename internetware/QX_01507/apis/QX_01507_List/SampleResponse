<script Language="JavaScript">
function submitForm(act,param)
{
	currentPage     = parseInt(form1.currentPageNo.value);
	maxRecNoPerPage = parseInt(form1.maxRecNoPerPage.value);
	recCount        = parseInt(form1.recordNumber.value);

	if(act == "prev") { 
		currentPage = currentPage - 1;
		if(currentPage < 1 ) {
			alert("无法跳转到该页面，页号出界！");
			return;
		}else{
			form1.currentPageNo.value = currentPage;
		}
	}
	if(act == "next"){	
		if ((currentPage * maxRecNoPerPage )>= recCount){
			alert("无法跳转到该页面，页号出界！");
			return;
		}else{
			currentPage = currentPage + 1 ;
			form1.currentPageNo.value = currentPage;
		}
	}
	if (act == "goto") {
		form1.currentPageNo.value = form1.toPage.value;
	}
	window.location="/level2.jsp?caid=011-002&topage=" + form1.currentPageNo.value;
}
</script>
  
 
  


